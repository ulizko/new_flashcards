<div class='row'>
  <div class='col-md-12'>
    <h3 class="text-center">Visits from countries</h3>
    <%= geo_chart Visit.group(:country).count, label: "Visits" %>
  </div>
</div>
<div class='row'>
  <div class='col-md-6'>
    <h3 class="text-center">Loads image from Flickr</h3>
    <%= column_chart loads_images_from_flickr_statistics_path, label: "Count", ytitle: "Images" %>
  </div>
  <div class='col-md-6'>
    <h3 class="text-center">Visits per day </h3>
    <%= line_chart visits_use_social_statistics_path, label: "Visits", ytitle: "Visits" %>
  </div>
</div>
<div class='row'>
  <div class='col-md-12'>
    <h3 class="text-center">Results review cards per day</h3>
    <%= column_chart results_review_cards_statistics_path, stacked: true, ytitle: "Results" %>
  </div>
</div>